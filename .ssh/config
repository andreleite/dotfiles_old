Host *
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  ServerAliveInterval 60

Host *andre.io
  ForwardAgent yes

Host dev
  HostName dev.andre.io
  User ubuntu
  IdentityFile ~/keys/andre.io

Host github.com
  HostName github.com
  User git
  IdentityFile ~/keys/andre.io

host gitlab.com
  hostname gitlab.com
  user git
  identityfile ~/keys/andre.io

Host devel4
  HostName devel4.prod.lan.com
  User aleite
  IdentityFile ~/keys/andre.io
